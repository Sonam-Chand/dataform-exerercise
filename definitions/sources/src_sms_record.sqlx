config {
    type: "view",
    description: "Holds the sms_records of each subscriber.",
    tags: ["sms_record","source"],
    assertions: {
        nonNull: ["subscription_id"],
        
    }
}

select *
        from
            external_query(
                "${constants.external_connection.cloud_sql}",
                "select * from activity.sms_record;"
            )
        limit 1000
